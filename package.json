{
	"name": "twelve-to-twentyfour",
	"version": "0.1.0",
	"description": "A boilerplate for creating a browser extension with webpack",
	"author": "Nicolas Newman <nanewman01@gmail.com>",
	"scripts": {
		"build": "npm-run-all -s clean build:*",
		"build:global": "webpack --config webpack.config.js --config-name global",
		"build:action": "webpack --config webpack.config.js --config-name action",
		"build:options": "webpack --config webpack.config.js --config-name options",
		"clean": "rimraf dist/",
		"start": "web-ext run --source-dir ./dist/"
	},
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/preset-env": "^7.12.1",
		"babel-loader": "^8.1.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^6.2.1",
		"css-loader": "^5.0.0",
		"file-loader": "^6.1.1",
		"html-loader": "^1.3.2",
		"html-webpack-plugin": "^4.5.0",
		"less": "^3.12.2",
		"less-loader": "^7.0.2",
		"mini-css-extract-plugin": "^1.2.0",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"style-loader": "^2.0.0",
		"url-loader": "^4.1.1",
		"web-ext": "^5.3.0",
		"webpack": "^5.2.0",
		"webpack-cli": "^4.1.0",
		"webpack-sources": "^2.0.1"
	},
	"dependencies": {}
}
